{
	"manifest_version": 2,
	"name": "TabsWQ!",
	"version": "0.2",
	"description": "Saving Chrome tabs schedulely",
	"background": {
		"scripts": [
			"include/jquery-1.11.0.min.js",	
			// "scripts/bg.js",
			"scripts/main.js"
		],
		"persistent": true
	},
	"browser_action": {
		"default_title": "TabsWQ",
		"default_popup": "popup.html"
	},
	/*"content_scripts": [{
		"matches": [
			"https://*.baidu.com/*"
		],
		"css": ["css/common.css"],
		"js": [
			"include/jquery-1.11.0.min.js",
			"scripts/mm.js"
		]
	}],*/
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"permissions": [
        "tabs",
        "storage",
        "downloads"
    ]
}
